{
    // ------------------------------------------------------------------------
    // Visual Studio Code: Window
    // ------------------------------------------------------------------------
    "window.zoomLevel": 0,
    // ------------------------------------------------------------------------
    // Visual Studio Code: Editor
    // ------------------------------------------------------------------------
    "editor.fontFamily": "FiraCode-Retina",
    "editor.fontSize": 13,
    "editor.fontWeight": "400",
    "editor.fontLigatures": true,
    "editor.lineHeight": 0, // 0==20sc
    "editor.rulers": [
        85,
        100
    ],
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 2,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorSurroundingLines": 2,
    "editor.cursorSurroundingLinesStyle": "all",
    "editor.renderIndentGuides": true,
    "editor.minimap.enabled": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.maxColumn": 100,
    "editor.minimap.showSlider": "always",
    "editor.showFoldingControls": "mouseover",
    "editor.formatOnSave": true,
    // ------------------------------------------------------------------------
    // Visual Studio Code: Explorer
    // ------------------------------------------------------------------------
    "explorer.confirmDragAndDrop": false,
    "editor.tabSize": 2,
    "explorer.confirmDelete": false,
    // ------------------------------------------------------------------------
    // Visual Studio Code: Workbench
    // ------------------------------------------------------------------------
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Brackets Light Pro Bold",
    // ------------------------------------------------------------------------
    // Visual Studio Code: Files
    // ------------------------------------------------------------------------
    "files.trimTrailingWhitespace": true,
    // ------------------------------------------------------------------------
    // Visual Studio Code: SCM & Git
    // ------------------------------------------------------------------------
    "scm.diffDecorationsGutterWidth": 5,
    "diffEditor.ignoreTrimWhitespace": false,
    "git.path": null,
    "git.autofetch": true,
    // ------------------------------------------------------------------------
    // GitLense
    // ------------------------------------------------------------------------
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "gitlens.views.repositories.files.layout": "list",
    "gitlens.mode.active": "zen",
    // ------------------------------------------------------------------------
    // AutoDocstring
    // ------------------------------------------------------------------------
    "autoDocstring.docstringFormat": "numpy",
    "autoDocstring.startOnNewLine": true,
    // ------------------------------------------------------------------------
    // ToDo Hightlight
    // ------------------------------------------------------------------------
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss",
        "**/*.rs",
        "**/*.py"
    ],
    "todohighlight.keywords": [
        {
            "text": "BUG:",
            "color": "#000000",
            "border": "1px solid #000000",
            "borderRadius": "0px",
            "backgroundColor": "#FF0000",
            "isWholeLine": true,
            "overviewRulerColor": "#FF0000"
        },
        {
            "text": " TODO: ",
            "color": "#FF0000",
            "border": "1px solid #FF0000",
            "borderRadius": "0px",
            "backgroundColor": "rgba(0,0,0,.03)",
            "isWholeLine": false,
            "overviewRulerColor": "#FF0000"
        },
        {
            "text": " OPTIMIZE: ",
            "color": "#2196f3",
            "border": "1px solid #2196f3",
            "borderRadius": "0px",
            "backgroundColor": "rgba(0,0,0,.03)",
            "isWholeLine": false,
            "overviewRulerColor": "#2196f3"
        },
        {
            "text": " QUESTION: ",
            "color": "#2196f3",
            "border": "1px solid #2196f3",
            "borderRadius": "0px",
            "backgroundColor": "rgba(0,0,0,.03)",
            "isWholeLine": false,
            "overviewRulerColor": "#2196f3"
        },
        {
            "text": " REFACTOR: ",
            "color": "#760CE8",
            "border": "1px solid #760CE8",
            "borderRadius": "0px",
            "backgroundColor": "rgba(0,0,0,.03)",
            "isWholeLine": false,
            "overviewRulerColor": "#760CE8"
        },
        {
            "text": " TEST: ",
            "color": "#FF970D",
            "border": "1px solid #FF970D",
            "borderRadius": "0px",
            "backgroundColor": "rgba(0,0,0,.03)",
            "isWholeLine": false,
            "overviewRulerColor": "#FF970D"
        },
        {
            "text": " DONE: ",
            "color": "#51D800",
            "border": "1px solid #51D800",
            "borderRadius": "0px",
            "backgroundColor": "rgba(0,0,0,.03)",
            "isWholeLine": false,
            "overviewRulerColor": "#51D800"
        },
        {
            "text": " NOTE: ",
            "color": "grey",
            "border": "1px solid grey",
            "borderRadius": "0px",
            "backgroundColor": "#FFF60D",
            "isWholeLine": false,
            "overviewRulerColor": "grey"
        },
    ],
    // ------------------------------------------------------------------------
    // Rust
    // ------------------------------------------------------------------------
    "rust-client.logToFile": false,
    "rust.build_on_save": true,
    "rust.wait_to_build": null,
    "rust.full_docs": null,
    "rust.jobs": 4,
    "rust.show_hover_context": true,
    // ------------------------------------------------------------------------
    // Python
    // ------------------------------------------------------------------------
    "python.pythonPath": "/Users/tillmannradmer/miniconda3/envs/ml/bin/python",
    "python.sortImports.path": "",
    "python.linting.enabled": true,
    "python.linting.pylintArgs": [
        // "--indent-string=\"  \"",
        // "--indent-after-paren=2",
        "--generated-members=numpy.* ,torch.* ,cv2.* , cv.*"
    ],
    "python.autoComplete.extraPaths": [],
    "python.formatting.provider": "autopep8",
    "python.condaPath": "/Users/tillmannradmer/miniconda3/bin/conda",
    "python.formatting.autopep8Args": [
        // "--indent-size=2",
        "--ignore E121"
    ],
    // ------------------------------------------------------------------------
    // C#
    // ------------------------------------------------------------------------
    "omnisharp.projectLoadTimeout": 240,
    "omnisharp.loggingLevel": "warning",
    "csharp.referencesCodeLens.enabled": false,
    "markdown.extension.toc.githubCompatibility": true,
    "markdown.extension.toc.levels": "2..6",
    "markdown.extension.toc.orderedList": true,
    // ------------------------------------------------------------------------
    // Go
    // ------------------------------------------------------------------------
    "go.formatTool": "goimports",
    "go.useLanguageServer": false,
    // ------------------------------------------------------------------------
    // AUTOMATICALLY ADDED FIELDS
    // ------------------------------------------------------------------------
}